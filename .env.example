# Copy this to .env and fill in the Moondream API key provided in your dashboard.
# Vite only exposes variables prefixed with VITE_ to the client, so we mirror the
# same value to VITE_MOONDREAM_API_KEY for runtime access.
MOONDREAM_API_KEY=your-moondream-api-key
VITE_MOONDREAM_API_KEY=${MOONDREAM_API_KEY}
